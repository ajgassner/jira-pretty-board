{
  "/jira-api": {
    "target": {
      "host": "yourJiraInstance.atlassian.net",
      "protocol": "https:",
      "port": 443
    },
    "secure": false,
    "pathRewrite": {
      "^/jira-api": "/rest"
    },
    "changeOrigin": true,
    "logLevel": "debug",
    "auth": "yourUser:yourPassword"
  }
}
